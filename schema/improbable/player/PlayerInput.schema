package improbable.player;

type Joystick {
    float x_axis = 1;
    float y_axis = 2;
}

type Fire {}
type Move {}
type Stop {}

component PlayerInput {
    id = 1004;
    Joystick joystick = 1;
    // The component event for triggering firing.
  	event Fire fire;
	event Move move;
	event Stop stop;
}
